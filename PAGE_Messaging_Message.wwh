      PROCEDURE PAGE_Messaging_Message(gnMessageID)
      
      gbMustClose is boolean 
      
      // Retrieves the message
      HReadSeekFirst(Message, MessageID, gnMessageID)
      IF HFound(Message) THEN
      	
      	// Displays the message
      	LoadMessage()	
      	
      ELSE
      	
      	// Message not found
      	Error(<§$0000§>)
      	gbMustClose = True
      	
      END
