      PROCEDURE InitializeActions(sCombo)
      
      
      ListDeleteAll(sCombo)
      
      // Simple actions
      ListAdd(sCombo, gLink(-1) + <§$0001§>)
      ListAdd(sCombo, gLink(-2) + RepeatString(SPC, 5) + <§$0002§>)
      ListAdd(sCombo, gLink(-3) + RepeatString(SPC, 5) + <§$0003§>)
      
      // Apply the labels?
      QRY_Messaging_Labels.pUserID = CurrentUser:UserID
      HExecuteQuery(QRY_Messaging_Labels)
      
      IF HNbRec(QRY_Messaging_Labels) > 0 THEN
      	
      	ListAdd(sCombo, gLink(-4) + <§$0004§>)	
      	ListAdd(sCombo, gLink(-1) + <§$0005§>)
      	
      	// Adds the labels
      	FOR EACH QRY_Messaging_Labels
      		ListAdd(sCombo, gLink(QRY_Messaging_Labels.LabelID) + RepeatString(SPC, 5) + QRY_Messaging_Labels.Caption)
      	END
      END
      
      sCombo..State = Grayed
      
      
