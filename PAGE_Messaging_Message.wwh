     name : UniformMessage
     procedure_id : 1825671716568938688
     type_code : 14
     code : |1-
      // Summary: Transforms a source message (HHCode, HTML or rough text) into a uniform HTML message
      // Syntax:
      //[ <Result> = ] UniformMessage (<sMessage>)
      //
      // Parameters:
      //	sMessage: Initial message
      // Return value:
      // 	string: Uniform message
      //
      PROCEDURE UniformMessage(sMessage)
      
      sUnifMessage is string
      sUnifMessage = sMessage
      
      SWITCH True 
      	// Checks whether it is rough text
      	CASE HTMLToText(sUnifMessage) = sUnifMessage
      		
      		sUnifMessage = "<p class=Simple>" + TextToHTML(sUnifMessage) + "</p>"
      		
      	OTHER CASE
      		
      		// It is HTML
      		
      		
      END
      
      
      RESULT sUnifMessage
     type : 458752
