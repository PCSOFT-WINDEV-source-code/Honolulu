        code : |1-
         
         // Checks whether the message was sent by the current user
         HReadSeekFirst(Message, MessageID, gnMessageID)
         IF HFound(Message) THEN
         	
         	// The message was sent by the current user
         	IF Message.SenderID = CurrentUser:UserID THEN
         		
         		// Mark this message as deleted
         		IF PAGE_MessagingPortal.gnList = BoxDeleted THEN
         			Message.IsDeleted = False
         		ELSE
         			IF PAGE_MessagingPortal.gnList = BoxSent THEN
         				// Don't delete the message sent
         				Message.IsDeleted = True
         			END
         		END			
         		
         		HModify(Message)
         	END
         	
         	// Search for the current message in the recipients
         	FOR EACH Recipient WHERE UserIDByMessageID = [CurrentUser:UserID, gnMessageID]
         		
         		// Mark this message as deleted
         		IF PAGE_MessagingPortal.gnList = BoxDeleted THEN
         			Recipient.IsDeleted = False
         		ELSE
         			IF PAGE_MessagingPortal.gnList <> BoxSent THEN
         				// Don't delete the message sent
         				Recipient.IsDeleted = True
         			END
         		END	
         		
         		HModify(Recipient)
         	END
         	
         	
         	EDT_CLOSE = 1
         	
         END
