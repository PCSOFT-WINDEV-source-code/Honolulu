     name : LoadMessage
     internal_properties : 
     procedure_id : 1825671716568873152
     type_code : 14
     code : |1+
      // Summary: Loads the current message
      // Syntax:
      // LoadMessage ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE LoadMessage()
      
      sSender			is string
      sListRecipients is string
      sListAttachments is string
      sSignature			is string
      sHTMAttachments 	is string
      
      
      // Retrieves the recipients
      FOR EACH Recipient WHERE MessageID = gnMessageID
      	
      	// Don't display the hidden recipients
      	IF Recipient.IsHiddenRecipient THEN CONTINUE
      	
      	// Ignore the current user
      	IF Recipient.UserID = CurrentUser:UserID THEN 
      		// First recipient: current user
      		sListRecipients += [", "] + <§$0022§>
      		CONTINUE
      	END
      	
      	// Retrieves the user
      	IF Recipient.UserID <> -1 THEN
      		// The user is a known user, display his name
      		sListRecipients += [", "] + GetUserName(Recipient.UserID)
      	ELSE
      		// The user is an external user, display his email
      		sListRecipients += [", "] + GetNameDisplayedEmailSnd(Recipient.RecipientEmail)
      	END
      	
      END
      
      // By default, the message is necessarily sent to the user
      IF sListRecipients ~= "" THEN
      	sListRecipients = <§$0023§>
      END
      
      // Sender of the message
      IF Message.SenderID = -1 THEN
      	sSender = GetNameDisplayedEmail(Message.SenderEmail)
      ELSE
      	sSender = GetUserName(Message.SenderID)
      	sSignature  = HHCodeToHTML(GetUserSignature(Message.SenderID, True))
      END
      
      // Retrieves the attachments
      FOR EACH MessageAttach WHERE MessageID = gnMessageID
      	
      	sListAttachments += [CR] + MessageAttach.Key
      	
      END
      
      //
      // Display
      //
      
      // Sender and recipients
      HTMSTC_SENDREC = StringBuild(HTMSTC_SENDREC, sSender, sListRecipients)
      
      // Subject
      IF Message.Subject ~= "" THEN Message.Subject = <§$0006§>
      STC_SUBJECT_COL	   = Message.Subject
      MyPage..Title  = "Honolulu - " + Message.Subject
      
      // Date
      dtDateTimeReception is datetime
      dtDateTimeReception = Message.OUTGOINGDATETIME
      
      IF dtDateTimeReception..Date = Today THEN
      	// Display the time
      	STC_DATE = TimeToString(dtDateTimeReception..Time, "HH:MM AP") + StringBuild(" (%1)", NaturalDateTime(dtDateTimeReception))
      ELSE
      	STC_DATE = NaturalDateTime(dtDateTimeReception, True)
      END
      
      // Message
      sMessage is string
      sMessage = Message.Content
      
      // Transforms the message if necessary and add the signature
      sMessage = UniformMessage(sMessage) + sSignature
      
      // Add the attachments to the message
      IF sListAttachments <> "" THEN
      	
      	nNbAttachments is int
      	sKey, sURL is string
      	sInfo, sFileName, sMIME are strings
      	cySize is currency
      	
      	// For each attachment
      	FOR EACH string sKey of sListAttachments SEPARATED BY CR
      		
      		// Check whether the attachment still exists
      		IF ATTACHKeyExist(sKey) THEN
      			
      			// Retrieves all the details about the attachment
      			sURL = ATTACHGetURL(sKey)
      			ATTACHGet(sKey, sInfo, sMIME, cySize, sFileName)
      			
      			// If it's an image
      			IF Position(sMIME, "image", 0, IgnoreCase) > 0 THEN
      				
      				// Display a preview of the image
      				sHTMAttachments += ["<br/>" + CR] + "<table border=0 cellpadding=2 cellspacing=2><tr><td width=100 height=100 style=""border:1px solid #c0c0c0"">" + ...
      				StringBuild("<a href=""javascript:PHONOLULUBRW_DOWNLOAD('%1');""><img src=""" + sURL + "&mode=homo&sizel=100&sizeh=100"" width=100 height=100 border=0 /></a></td>", sKey) + ...
      				"<td width=10>&nbsp;</td>" +  ...
      				"<td class=Simple>" + StringBuild("<strong>%1</strong>", sFileName) + ...
      				"<br/>" + CR +  LengthToString(cySize) + SPC + ...
      				StringBuild(<§$000e§>, sKey) + "</td></tr></table>"
      				
      			ELSE
      				
      				// Display a link to the file
      				sHTMAttachments += ["<br/>" + CR] + "<p><span class=Simple>" + StringBuild("<img src=""%2"" width=16 height=16 />", sFileName, "/" + FolderWeb() + ...
      				"/UK/PAGE_ATTACH." + AWP_EXT + "?mime=" + fExtractPath(sFileName, fFileName+fExtension) + "&sizel=16&sizeh=16&AWPSESSIONID=" + gsIdentifier) + SPC + ...
      				StringBuild("<strong>%1</strong>", sFileName) + ...
      				"<br/>" + CR +  LengthToString(cySize) + SPC + ...
      				StringBuild(<§$0016§>, sKey) + "spanan></p>"	
      			END
      			
      			// Count the attachments
      			nNbAttachments ++
      			
      		END
      		
      	END
      	
      	sHTMAttachments = StringBuild("<hr/><br/><div class=Simple><strong>%1</strong><br/><br/>%2</div>", StringBuild(<§$0019§>, nNbAttachments, (nNbAttachments > 1 ? "s" ELSE "")), sHTMAttachments)	
      END
      
      HTMSTC_MESSAGE..Value = StringBuild("<div style=""position: relative; width:100%""><table align=left valign=top><tr><td>%1</td></tr></table><br></div>", sMessage + sHTMAttachments)
      
      // We do not have to answer to the service of notifications
      IF Message.SenderID = SYSTEMID() THEN
      	LNK_RESPOND_MSG..State = Grayed
      	LNK_RESPONDTOALL_MSG..State = Grayed
      ELSE
      	LNK_RESPOND_MSG..State = Active
      	LNK_RESPONDTOALL_MSG..State = Active
      END
      
      IF Message.IsDeleted THEN
      	LNK_DELETE_MSG = <§$001c§>
      	LNK_DELETE_MSG..ToolTip = <§$001d§>
      ELSE
      	LNK_DELETE_MSG = <§$001e§>
      	LNK_DELETE_MSG..ToolTip = <§$001f§>
      END
      
     type : 458752
